<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RectProgressView">
        <!--进度条背景颜色-->
        <attr name="shapeBgColor" format="color"/>
        <!--文字颜色-->
        <attr name="textColor" format="color"/>
        <!--线条颜色-->
        <attr name="lineColor" format="color"/>
        <!--进度一颜色-->
        <attr name="progressOneColor" format="color"/>
        <!--进度二颜色-->
        <attr name="progressTwoColor" format="color"/>
        <!--进度三颜色-->
        <attr name="progressThreeColor" format="color"/>
        <!--进度四颜色-->
        <attr name="progressFourColor" format="color"/>
        <!--类型一名称-->
        <attr name="typeOneName" format="string"/>
        <!--类型二名称-->
        <attr name="typeTwoName" format="string"/>
        <!--类型三名称-->
        <attr name="typeThreeName" format="string"/>
        <!--类型四名称-->
        <attr name="typeFourName" format="string"/>
        <!--文字大小-->
        <attr name="textSize" format="dimension"/>
        <!--线条大小-->
        <attr name="lineSize" format="dimension"/>
    </declare-styleable>


    <declare-styleable name="CircularProgressView">

        <!--进度一颜色-->
        <attr name="ProgressFirstColor" format="color"/>
        <!--进度二颜色-->
        <attr name="ProgressSecondColor" format="color"/>
        <!--装饰圆颜色-->
        <attr name="DecorateCircularColor" format="color"/>

    </declare-styleable>






    <declare-styleable name="SpreadView">
        <!--中心圆颜色-->
        <attr name="spread_center_color" format="color" />
        <!--中心圆半径-->
        <attr name="spread_radius" format="integer" />
        <!--扩散圆颜色-->
        <attr name="spread_spread_color" format="color" />
        <!--扩散间距-->
        <attr name="spread_distance" format="integer" />
        <!--扩散最大半径-->
        <attr name="spread_max_radius" format="integer" />
        <!--扩散延迟间隔-->
        <attr name="spread_delay_milliseconds" format="integer" />
    </declare-styleable>


        <declare-styleable name="MZBannerView">
            <attr name="open_mz_mode" format="boolean"/>
            <attr name="canLoop" format="boolean"/>
            <attr name="indicatorPaddingLeft" format="dimension"/>
            <attr name="indicatorPaddingRight" format="dimension"/>
            <attr name="indicatorPaddingTop" format="dimension"/>
            <attr name="indicatorPaddingBottom" format="dimension"/>
            <attr name="indicatorAlign">
                <enum name="left" value="0"/>
                <enum name="center" value="1"/>
                <enum name="right" value="2"/>
            </attr>
            <attr name="middle_page_cover" format="boolean"/>
        </declare-styleable>

</resources>